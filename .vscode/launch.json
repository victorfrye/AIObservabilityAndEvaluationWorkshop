    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/AIObservabilityAndEvaluationWorkshop.AppHost/bin/Debug/net10.0/AIObservabilityAndEvaluationWorkshop.AppHost.dll",
            "args": [],
            "cwd": "${workspaceFolder}/AIObservabilityAndEvaluationWorkshop.AppHost",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "DOTNET_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "https://localhost:17105;http://localhost:15048",
                "ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "https://localhost:21023",
                "ASPIRE_RESOURCE_SERVICE_ENDPOINT_URL": "https://localhost:22105"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}